{
  "name": "muscles-framework",
  "version": "0.0.1",
  "description": "Simple, lightweight front-end framework based on Skeleton",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:sass": "node-sass --source-map true src/scss -o dist/css",
    "build:js": "webpack --mode=development",
    "build:all": "npm run build:sass && npm run build:js",
    "watch:sass": "node-sass -rw src/scss -o dist/css",
    "watch:js": "webpack --watch --mode=development",
    "watch:all": "run-p watch:sass watch:js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonsbarr/muscles-framework"
  },
  "keywords": [
    "front-end",
    "skeleton",
    "skeleton-framework",
    "skeleton-css",
    "muscles",
    "muscles-framework",
    "vanilla-javascript",
    "scss"
  ],
  "author": "Jason Barr <jason@jasonsbarr.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsbarr/muscles-framework/issues"
  },
  "homepage": "https://github.com/jasonsbarr/muscles-framework#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "browser-sync": "^2.24.5",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  }
}
